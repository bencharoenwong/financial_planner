{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "docs/**/*.md",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api.py"
    },
    {
      "src": "/docs/(.*\\.md)",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "functions": {
    "api.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  }
}
